{
  "crons": [
    { "path": "/api/cron/cal-cache", "schedule": "*/10 * * * *" },
    { "path": "/api/cron/frequent", "schedule": "*/15 * * * *" },
    { "path": "/api/cron/cal-followups", "schedule": "*/30 * * * *" },
    { "path": "/api/cron/business-hours", "schedule": "0 7-20 * * *" },
    { "path": "/api/cron/morning-alerts", "schedule": "0 8 * * *" },
    { "path": "/api/cron/lead-nurturing", "schedule": "0 9 * * *" },
    { "path": "/api/cron/lead-nurturing-late", "schedule": "30 10 * * *" },
    { "path": "/api/admin/alerts/new-leads?hours=6", "schedule": "0 14 * * *" },
    { "path": "/api/cron/evening-alerts", "schedule": "0 20 * * *" },
